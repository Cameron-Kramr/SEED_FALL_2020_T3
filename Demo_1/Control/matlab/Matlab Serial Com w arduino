%%
clear 
port='COM8';% changes based on system
obj = serial(port, 'BaudRate', 250000);
obj.terminator = 'CR/LF';
if isempty(instrfind) %put a ~ before isempty run then remove the ~ and run again that should get the code to work
     fclose(instrfind);
      delete(instrfind);
end
fopen(obj);
for k =1:600
Velocity=fgetl(obj);
cCheck=convertCharsToStrings(Velocity);
RadVel(k)=cCheck;
display(Velocity);
end
fclose(obj);
%%
AngleVel_A=str2double(RadVel(1:2:end));
AngleVel_B=str2double(RadVel(2:2:end));
r=2.88/12; %radius in feet of each wheel
InstantVel_A=r*(AngleVel_A); v=wr
InstantVel_B=r*(AngleVel_B); v=wr
figure(1)
plot(InstantVel_A)
figure(2)
plot(InstantVel_B)
